APP=mkl-node
CUSER=${GITUSER}
KEYNAME=${GITKEYNAME}
KEYPATH=${GITKEYPATH}
LOCALUSER="--arg=LOCALUSER=${USER}"
_USER_="-u ${CUSER}"
RUNENV="-e='DOCKER_ENV=${APP}'"
REMOVE=true
PRIV=true
PURPOSE=application
ENV=run
OS=ubuntu
PORT="-p=8080:8080"
RUNSTATE="-I --name=sharkdemo"
RUNENV="${RUNENV} -v=sharkdemo:/sharkdemo"
